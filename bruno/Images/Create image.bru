meta {
  name: Create image
  type: http
  seq: 3
}

post {
  url: {{api}}/api/images
  body: multipartForm
  auth: inherit
}

body:multipart-form {
  image: @file(/home/aleygues/Images/coding.jpg)
  name: Une super image
  description: Elle est belle cette image
  lng: 4
  lat: 4
  author: John
}

body:file {
  file: @file(/home/aleygues/Images/coding2.jpg) @contentType(image/jpeg)
}

settings {
  encodeUrl: true
  timeout: 0
}
